<div class="container-fluid">
  <div class="interesting">
    <h4>{{ text_other_projects }}</h4>
    <div class="projects-list">
      {% for information in informations %}
        <div class="item">
          <a href="{{ information.href }}">
            <div class="image">
              <img class="lazyload" alt="1" data-src="{{ information.original_image }}">
            </div>
            <div class="overlay">
              <div class="city">
                {{ information.city }}
              </div>
              <div class="title">
                {{ information.title }}
              </div>
              <hr>
            </div>
          </a>
        </div>
      {% endfor %}
    </div>
  </div>
</div>
